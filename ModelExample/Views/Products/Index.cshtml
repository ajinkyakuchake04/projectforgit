@*Since we are passing a List of Object do as following*@

@model List<ModelExample.Models.Product>

@{
    ViewBag.Title = "Index";
}

<h2>Products</h2>
<a href="@Url.Action("Create","Products")">Create Product</a>
<table class="table">
    <thead>
        <tr>
            <th>Product Id</th>
            <th>Product Name</th>
            <th>Rate</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ProductId</td>
                <td><a href="/products/details/@item.ProductId">@item.ProductName</td>
                <td>@item.Rate</td>
            </tr>
        }

    </tbody>
</table>

